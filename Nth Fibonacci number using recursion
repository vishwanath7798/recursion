import java.util.*;
public class Main
{
    public static int fibonacci(int n){
        if(n==0 || n==1){
            return n;
        }
        int fnm1=fibonacci(n-1);
        int fnm2=fibonacci(n-2);
        int f3=fnm1+fnm2;
        return f3;
    }
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		System.out.print(fibonacci(n));
	}
}
